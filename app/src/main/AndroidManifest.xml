<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.FitCom.fitcomapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FitComApplication">
        <activity android:name=".TrainerActivity"></activity>
        <activity
            android:name=".SettingScreen.SettingActivity"
            android:parentActivityName=".HomePageActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".HomePageActivity" />
        </activity>
        <activity android:name=".BlogScreen.BlogActivity" />
        <activity android:name=".NutritionScreen.NutritionsActivity" />
        <activity android:name=".ExerciseScreen.ExerciseActivity" />
        <activity android:name=".HomePageActivity" />
        <activity android:name=".Registery.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>